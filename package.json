{
  "name": "vscode-code-review",
  "displayName": "code-review",
  "description": "Create expert reviews / code reviews for a workspace that can be exported as a document for handing over to customers",
  "version": "0.0.1",
  "author": "Danny Koppenhagen <mail@d-koppenhagen.de> (https://d-koppenhagen.de)",
  "homepage": "https://github.com/d-koppenhagen/vscode-code-review",
  "bugs": {
    "url": "https://github.com/d-koppenhagen/vscode-code-review/issues",
    "email": "mail@d-koppenhagen.de"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-review.addNote"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-review.addNote",
        "title": "Code Review: Add Note"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "",
          "command": "code-review.addNote",
          "group": "cmdGroup@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "standard-version": "^8.0.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
